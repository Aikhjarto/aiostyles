####normal streets
highway=trunk {name '${name} (${ref})' | '${ref}' | '${name}'} [0x03 road_class=1 road_speed=1 resolution 14]
highway=primary {name '${name} (${ref})' | '${ref}' | '${name}'} [0x04 road_class=1 road_speed=1 resolution 16]
highway=primary_cycle {name '${name} (${ref})' | '${ref}' | '${name}'} [0x04 road_class=2 road_speed=2 resolution 16]
highway=primary_ol {name '${name} (${ref})' | '${ref}' | '${name}'} [0x04 road_class=0 road_speed=1 resolution 16]
highway=secondary {name '${name} (${ref})' | '${ref}' | '${name}'} [0x05 road_class=1 road_speed=2 resolution 18]
highway=secondary_cycle {name '${name} (${ref})' | '${ref}' | '${name}'} [0x05 road_class=2 road_speed=3 resolution 18]
highway=secondary_ol {name '${name} (${ref})' | '${ref}' | '${name}'} [0x05 road_class=0 road_speed=1 resolution 18]
highway=tertiary {name '${name} (${ref})' | '${ref}' | '${name}'} [0x06 road_class=2 road_speed=3 resolution 19]
highway=tertiary_cycle {name '${name} (${ref})' | '${ref}' | '${name}'} [0x06 road_class=2 road_speed=4 resolution 19]
highway=tertiary_ol {name '${name} (${ref})' | '${ref}' | '${name}'} [0x06 road_class=0 road_speed=1 resolution 19]
highway=unclassified & area!=yes {name '${name} (${ref})' | '${ref}' | '${name}'} [0x07 road_class=4 road_speed=5 resolution 20]
highway=residential & area!=yes {name '${name} (${ref})' | '${ref}' | '${name}'} [0x08 road_class=4 road_speed=5 resolution 22]
highway=living_street & area!=yes {name '${name} (${ref})' | '${ref}' | '${name}'} [0x08 road_class=3 road_speed=5 resolution 22]
highway=service & area!=yes {name '${name} (${ref})' | '${ref}' | '${name}'} [0x08 road_class=3 road_speed=4 resolution 22]

####ways with access=no
( highway=footway | highway=byway | highway=pedestrian ) & area!=yes & indoor!=yes & access=no [0x10012 resolution 24]
highway=steps & area!=yes & indoor!=yes & access=no [0x10013 resolution 22]
highway=paved & area!=yes & indoor!=yes & access=no [0x10009 resolution 21]
highway=cobblestone & area!=yes & indoor!=yes & access=no [0x1000A resolution 22]
highway=normal & area!=yes & indoor!=yes & access=no [0x1000B resolution 21]
highway=unpaved & area!=yes & indoor!=yes & access=no [0x10010 resolution 21]
highway=bad & area!=yes & indoor!=yes & access=no [0x1000D resolution 22]
highway=path & area!=yes & indoor!=yes & access=no [0x1000E resolution 21]
highway=road & area!=yes & indoor!=yes & access=no [0x1001a resolution 22]

####ways
( highway=footway | highway=byway | highway=pedestrian ) & area!=yes & indoor!=yes [0x12 road_class=0 road_speed=0 resolution 24]
highway=steps & area!=yes & indoor!=yes [0x13 road_class=0 road_speed=0 resolution 22]
highway=paved & area!=yes & indoor!=yes [0x09 road_class=3 road_speed=7 resolution 21]
highway=cobblestone & area!=yes & indoor!=yes { set mkgmap:unpaved=yes } [0x0A road_class=0 road_speed=2 resolution 22]
highway=normal & area!=yes & indoor!=yes [0x0B road_class=1 road_speed=2 resolution 21]
highway=unpaved & area!=yes & indoor!=yes { set mkgmap:unpaved=yes } [0x10 road_class=0 road_speed=1 resolution 21]
highway=bad & area!=yes & indoor!=yes { set mkgmap:unpaved=yes } [0x0D road_class=0 road_speed=0 resolution 22]
highway=path & area!=yes & indoor!=yes { set mkgmap:unpaved=yes } [0x0E road_class=1 road_speed=2 resolution 21]
highway=road & area!=yes & indoor!=yes [0x11 road_class=1 road_speed=1 resolution 22]

###other routeable lines with access=no
man_made=pier & area!=yes & access=no [0x1000F resolution 24]
railway=platform & area!=yes & access=no [0x1000F resolution 24]

###other routeable lines
man_made=pier & area!=yes [0x0F road_class=1 road_speed=1 resolution 24]
railway=platform & area!=yes [0x0F road_class=1 road_speed=1 resolution 24]


###nonrouteable lines
railway=light_rail & tunnel!=yes [0x1C resolution 19]
railway=rail & tunnel!=yes & highway!=* [0x1C resolution 19]
